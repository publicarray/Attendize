APP_ENV=dev
APP_DEBUG=true
APP_NAME=attendize
APP_URL=http://attendize.test/
APP_KEY=
APP_TIMEZONE=

# choose between file, array or cookie
SESSION_DRIVER=file
SESSION_DOMAIN=
SESSION_SECURE_COOKIE=false
SESSION_ENCRYPTED=false
SESSION_LIFETIME=120
SESSION_COOKIE=attendize_session

DB_TYPE=pgsql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=attendize
DB_USERNAME=attendize
DB_PASSWORD=attendize

ENABLE_TEST_PAYMENTS=true

DEFAULT_DATE_FORMAT="Y-m-d"
DEFAULT_TIME_FORMAT="H:i"
DEFAULT_DATETIME_FORMAT="Y-m-d H:i"

APP_DEBUG_PDF=false
# https://github.com/NitMedia/wkhtml2pdf#driver-types
# chmod +x vendor/nitmedia/wkhtml2pdf/src/Nitmedia/Wkhtml2pdf/lib/wkhtmltopdf-0.12.1-OS-X.i386
WKHTML2PDF_BIN_FILE=wkhtmltopdf-0.12.1-OS-X.i386

MAIL_DRIVER=log
MAIL_FROM_ADDRESS=testing@attendize.test
MAIL_FROM_NAME=testing_service

GOOGLE_ANALYTICS_ID=
GOOGLE_MAPS_GEOCODING_KEY=

TWITTER_WIDGET_ID=

LOG=errorlog

